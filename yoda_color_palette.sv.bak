module yoda_color_palette (input logic [2:0] select_input,	
									
                           output logic [7:0] Red, Green, Blue
);
logic [7:0] Red, Green, Blue;

begin
	unique case(select_input)
	3'd1: 
		begin
			Red = 8'h;
			Green = 8'h;
			Blue = 8'h;
		end
	3'd2: 
		begin
			Red = 8'h;  
			Green = 8'h; 
			Blue = 8'h;
		end
	3'd3: 
